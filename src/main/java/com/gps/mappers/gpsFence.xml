<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gps.mappers.gpsFence">

  <insert id="saveFence" parameterType="String">
       insert into gpsFence(lonlat) values(#{lonlat})
  </insert>
    <delete id="delFence">
        truncate table gpsFence
    </delete>

</mapper>
